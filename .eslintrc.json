{
  "rules": {
    "max-lines-per-function": ["warn", 10],
    "no-var": 2,
    "max-depth": ["warn", 1],
    "no-undef": 2,
    "eqeqeq": "warn",
    "max-params" : ["warn", 2],
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
